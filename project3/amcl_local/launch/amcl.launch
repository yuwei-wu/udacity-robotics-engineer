


<launch>
<!-- TODO: Add nodes here -->



  <!-- Map Server -->
  <arg name="map_file" default="$(find amcl_local)/maps/studio.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />



  <!-- AMCL Node -->
  <node pkg="amcl" type="amcl" name="amcl" output="screen" >
    <remap from="scan" to="amcl_local/laser/scan"/>
    <param name="odom_frame_id" value="odom"/>
    <param name="odom_model_type" value="diff-corrected"/>
    <param name="base_frame_id" value="robot_footprint"/>
    <param name="global_frame_id" value="map"/>
  </node>


    <param name="initial_pose_x" value="0" />
    <param name="initial_pose_y" value="0" />


  <!-- Move Base -->
  <node name="move_base" pkg="move_base" type="move_base" respawn="false" output="screen">
  

  <remap from="scan" to="amcl_local/laser/scan"/>

  <param name="base_global_planner" value="navfn/NavfnROS" />
  <param name="base_local_planner" value="base_local_planner/TrajectoryPlannerROS"/>
  <rosparam file="$(find amcl_local)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
  <rosparam file="$(find amcl_local)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
  <rosparam file="$(find amcl_local)/config/local_costmap_params.yaml" command="load" />
  <rosparam file="$(find amcl_local)/config/global_costmap_params.yaml" command="load" />
  <rosparam file="$(find amcl_local)/config/base_local_planner_params.yaml" command="load" />
  </node>
</launch>
